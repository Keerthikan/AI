#import "map.txt" maps

task main()
{	
	FileOpenType file;
	file.Filename = "map.txt";
	SysFileOpenRead(file); 
	string width;
	string height;
	string diamod;
	string nope;
	if (file.Result == NO_ERR)
    	{
      		
		int count = 0;
		char temp;	
		fgets(width, 2, file.FileHandle);
		TextOut(0, LCD_LINE3, "done widht: " + width);	
		temp = fgetc(file.FileHandle);	
		fgets(height, 2, file.FileHandle);
 		TextOut(0, LCD_LINE4, "done height: " + height);
		temp = fgetc(file.FileHandle);		    
		fgets(diamod, 2, file.FileHandle);
		TextOut(0, LCD_LINE5, "done diamod: " + diamod);
	        //Wait(10000);
    	}
	ClearScreen();
	char map[10][9]; 
	fgets(nope,1000,file.FileHandle);
	while(file.Result == NO_ERR)
	{
	   for( int i = 0; i < StrToNum(height); i++ )
	   {
		
		 for(int j = 0 ; j < StrToNum(width) ; j++)
		{	
			char temps = fgetc(file.FileHandle);
			if(temps != '\n')
			{
				map[i][j] = temps;
				

			}
			else
			{
				//TextOut(0, LCD_LINE1,"Done" + NumToStr(i));
				//TextOut(0, LCD_LINE2,"Done: " + map[0]);
	   			//Wait(1000);
				ClearScreen();
			}
		}

		
	   }
	   //TextOut(0, LCD_LINE1,"Done" + NumToStr(i));
	   //string temp = new string(map[0]);
	   //TextOut(0, LCD_LINE2, temp);
	   Wait(10000);
	   ClearScreen();	
    	   break;
	}
	

}	
